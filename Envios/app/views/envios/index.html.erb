<p id="notice"><%= notice %></p>

<h1>Listing Envios</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @envios.each do |envio| %>
      <tr>
        <td><%= link_to 'Show', envio %></td>
        <td><%= link_to 'Edit', edit_envio_path(envio) %></td>
        <td><%= link_to 'Destroy', envio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Envio', new_envio_path %>
